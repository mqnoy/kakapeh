	<queryString>
		<![CDATA[SELECT 
transaksi.id_outlet,
outlet.nama_outlet,
transaksi.tgl_closing,
transaksi.t_transaksi,
pengeluaran.t_pengeluaran,
(transaksi.t_transaksi - pengeluaran.t_pengeluaran) as omsetbersih,
(transaksi.t_transaksi + pengeluaran.t_pengeluaran) as omsetkotor
FROM (
		SELECT id_outlet, tgl_closing,SUM(subtotal) as t_transaksi FROM tbl_transaksi WHERE id_outlet =$P{PARAM_ID_OUTLET} AND  tgl_closing  BETWEEN $P{PARAM_PERIOD_BEGIN} AND $P{PARAM_PERIOD_END}
	)  transaksi
	LEFT  JOIN 
	(
		SELECT id_outlet ,SUM(subtotal) as t_pengeluaran FROM tbl_pengeluaran WHERE id_outlet =$P{PARAM_ID_OUTLET} AND  tgl_pengeluaran BETWEEN $P{PARAM_PERIOD_BEGIN} AND $P{PARAM_PERIOD_END}
	) pengeluaran
	USING (id_outlet)
	LEFT JOIN 
	(
		SELECT id_outlet, nama_outlet FROM tbl_master_outlet tmo GROUP BY id_outlet 
	) outlet
	ON outlet.id_outlet = transaksi.id_outlet]]>
	</queryString>
